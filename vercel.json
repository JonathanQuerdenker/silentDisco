{
    "version": 2,
    "public": false,
    "rewrites": [{ "source": ".*", "destination": "/src" }],
    "routes": [
        { "src": ".*", "dest": "/public/index.html" },
        { "src": "/", "dest": "/" },
        { "src": "/guest-or-dj", "dest": "/src/components/modules/guest/ChooseRoom.js" },
        { "src": "/stage/:id", "dest": "/" },
        { "src": "/stage/", "dest": "/" },
        { "src": "/dj/:id", "dest": "/" },
        { "src": "/dj/", "dest": "/" }
    ]
}
